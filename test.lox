class Looper {
  init() {
    this.counts = range(255);
    for (var i = 0; i < len(this.counts); i = i + 1) {
      this.counts[i] = 0;
    }
  }
  get_input() {
    var usr_input = input();
    for (var i = 0; i < len(usr_input); i = i + 1) {
      this.counts[ord(usr_input[i])] = this.counts[ord(usr_input[i])] + 1;
    }
  }
  print_histogram() {
    for (var i = 0; i < len(this.counts); i = i + 1) {
      if (this.counts[i] > 0) {
        print chr(i);
        print this.counts[i];
      }
    }
  }

}

